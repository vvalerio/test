#########
# Operation
#########

<VONA/INGV/DDSS-032/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
        #end point precedente
        #hydra:template "http://193.206.223.51:8888/vona/volcano_name={volcano_name}/publication_date={publication_date}"^^xsd:string;
        #hydra:template "http://vo-tcs.ct.ingv.it/rest/vona/volcano_name={volcano_name}/publication_date={publication_date}"^^xsd:string;
        hydra:template "http://193.206.223.51/rest/vona/volcano_name={volcano_name}/publication_date={publication_date}"^^xsd:string;
        
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "volcano_name"^^xsd:string;
			rdfs:range "xsd:string" ;
			rdfs:label "Volcano name ";
			http:paramValue "Etna";
			http:paramValue "Stromboli";
			schema:defaultValue "Stromboli" ;
			hydra:required "false"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "publication_date"^^xsd:string;
			rdfs:range "xsd:dateTime" ;
			schema:valuePattern "YYYY-MM-DDThh:mm:ssZ";
			rdfs:label "Insert publication date in a standard format: (yyyy-mm-ddThh:mm:ssZ) ";
			schema:defaultValue "2019-07-30T00:00:00Z" ;
			hydra:required "false"^^xsd:boolean;
		];
	] ;
.
