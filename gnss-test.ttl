<GlassFramework/webresources/products/stations/operation> a hydra:Operation;
        hydra:method "GET"^^xsd:string;
        hydra:returns "application/json";
        hydra:property[ a hydra:IriTemplate;
        	#hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/stations{?analysis_center,reference_frame,otl_model,calibration_model,cut_of_angle,epoch_start,epoch_end,coordinates}"^^xsd:string;
