#WS Operation products/coordinates
<INGV/WS/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
  	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
    hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/{station}/INGV/daily/{coordinates_system}/{format}/{?reference_frame,otl_model,calibration_model,cut_of_angle,remove_outliers,apply_offsets,epoch_start,epoch_end}"^^xsd:string;
	
	hydra:mapping[ a hydra:IriTemplateMapping;
	    hydra:variable "station"^^xsd:string;
	    rdfs:label "Four letter station identification";
	    rdfs:range "xsd:string";
	    hydra:required "true"^^xsd:boolean;
	    schema:defaultValue "AQUI";
	];
    hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "coordinates_system"^^xsd:string;
	      rdfs:label "coordinates_system";
	      rdfs:range "xsd:string";
	      hydra:required "true"^^xsd:boolean;
	      http:paramValue "xyz";
	      http:paramValue "enu";
	      schema:defaultValue "xyz";
	];
	hydra:mapping[ a hydra:IriTemplateMapping;
	    hydra:variable "format"^^xsd:string;
	    rdfs:label "format; not all may be available";
	    rdfs:range "xsd:string";
	    hydra:required "true"^^xsd:boolean;
	    http:paramValue "midas";
		http:paramValue "xml";
		http:paramValue "json";
		http:paramValue "hector";
		http:paramValue "pbo";
		http:paramValue "covjson";
