 	schema:identifier "LTK/WS";
 	schema:description "Cleaned GNSS Position Time Series from LTK Web Service";
 	schema:name "Cleaned GNSS Position Time Series from LTK";
 	
	dcat:contactPoint <http://orcid.org/0000-0002-9733-284X/contactPoint> ;
 	schema:datePublished "2016-06-30T12:00:00Z"^^xsd:dateTime;
 	schema:dateModified "2016-12-31T12:00:00Z"^^xsd:dateTime;
	
  	# WP10-DDSS-006 Products.GNSS.Gateway UBI
	# UBI
	schema:provider <PIC:996437254>;
	dcat:theme  <epos:GNSSProductsPortal> ;
	hydra:entrypoint "https://glass.epos.ubi.pt:8080/GlassFramework/swagger.json"^^xsd:anyURI;
	schema:keywords "Geodesy", "GNSS" ;
	dct:conformsTo <GlassFramework/APIDocumentationProducts> ;
	hydra:supportedOperation <LTK/WS/operation>;
.

#########
# Operation
# Example  : returns data : https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/AQUI/LTK/daily/enu/covjson
######### 

<LTK/WS/operation> a hydra:Operation;
	hydra:method "GET"^^xsd:string;
  	hydra:returns "application/json";
	hydra:property[ a hydra:IriTemplate;
    hydra:template "https://glass.epos.ubi.pt:8080/GlassFramework/webresources/products/timeseries/{station}/LTK/{sampling_period}/{coordinates_system}/{format}/{?reference_frame,otl_model,calibration_model,cut_of_angle,remove_outliers,apply_offsets,epoch_start,epoch_end}"^^xsd:string;
	
	hydra:mapping[ a hydra:IriTemplateMapping;
	    hydra:variable "station"^^xsd:string;
	    rdfs:label "Four letter station identification";
	    rdfs:range "xsd:string";
	    hydra:required "true"^^xsd:boolean;
	    schema:defaultValue "AQUI";
	];
    hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "sampling_period"^^xsd:string;
	      rdfs:label "sampling_period";
	      rdfs:range "xsd:string";
	      hydra:required "true"^^xsd:boolean;
	      http:paramValue "daily";
	      http:paramValue "weekly";
	      schema:defaultValue "daily";
	];
    hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "coordinates_system"^^xsd:string;
	      rdfs:label "coordinates_system";
	      rdfs:range "xsd:string";
	      hydra:required "true"^^xsd:boolean;
	      http:paramValue "xyz";
	      http:paramValue "enu";
	      schema:defaultValue "xyz";
	];
	hydra:mapping[ a hydra:IriTemplateMapping;
	    hydra:variable "format"^^xsd:string;
	    rdfs:label "format; not all may be available";
	    rdfs:range "xsd:string";
	    hydra:required "true"^^xsd:boolean;
	    http:paramValue "midas";
		http:paramValue "xml";
		http:paramValue "json";
		http:paramValue "hector";
		http:paramValue "pbo";
		http:paramValue "covjson";
	    schema:defaultValue "covjson";
	];
    hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "reference_frame"^^xsd:string;
	      rdfs:label "reference_frame; use the attribute operation to get the all the reference frames available";
	      rdfs:range "xsd:string";
	      hydra:required "false"^^xsd:boolean;
	];
	hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "otl_model"^^xsd:string;
	      rdfs:label "otl_model; use the attribute operation to get the all the otl models available";
	      rdfs:range "xsd:string";
	      hydra:required "false"^^xsd:boolean;
    ];
	hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "calibration_model"^^xsd:string;
	      rdfs:label "calibration_model; use the attribute operation to get the all the calibration models available ";
	      rdfs:range "xsd:string";
	      hydra:required "false"^^xsd:boolean;
	];
	hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "cut_of_angle"^^xsd:string;
	      rdfs:label "cut_of_angle";
	      rdfs:range "xsd:float";
	      hydra:required "false"^^xsd:boolean;
	];
	hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "remove_outliers"^^xsd:string;
	      rdfs:label "remove_outliers";
	      schema:defaultValue "1";
	      rdfs:range "xsd:boolean";
	      hydra:required "false"^^xsd:boolean;
	      http:paramValue "1";
	      http:paramValue "0";
	];
	hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "apply_offsets"^^xsd:string;
	      rdfs:label "apply_offsets";
	      schema:defaultValue "0";
	      rdfs:range "xsd:boolean";
	      hydra:required "false"^^xsd:boolean;
	      http:paramValue "1";
	      http:paramValue "0";
	 ];
	 hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "epoch_start"^^xsd:string;
	      rdfs:label "epoch_start (date format YYYY-MM-DD)";
	      rdfs:range "xsd:dateTime";
	 hydra:property "schema:startDate";
	      schema:valuePattern "YYYY-MM-DD";
	      hydra:required "false"^^xsd:boolean;
	      schema:defaultValue "2013-01-01";
	 ];
	 hydra:mapping[ a hydra:IriTemplateMapping;
	      hydra:variable "epoch_end"^^xsd:string;
	      rdfs:label "epoch_end (date format YYYY-MM-DD)";
	      rdfs:range "xsd:dateTime";
	      hydra:property "schema:endDate";
	      schema:valuePattern "YYYY-MM-DD";
	      hydra:required "false"^^xsd:boolean;
	      schema:defaultValue "2017-12-31";
	];
];
.

